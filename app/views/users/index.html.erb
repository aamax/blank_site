<h1>All users</h1>

  <% if current_user.admin? %>
    <%= link_to "New User", signup_path %>
 <% end %>


<%= will_paginate %>

<ul class="users">

<table>  
  <% @users.each do |user| %>
    <% if user.fname != "aamaxtest" %>
      <% if current_user.admin? %>
        <%= render 'user_entry_admin', :object => user %>
      <% else %>
        <%= render 'user_entry', :object => user %>
      <% end %>  
     <% end %>
  <% end %>
</table>  

</ul>

<%= will_paginate %>

